{
  "name": "dfda-wiki-backup",
  "version": "1.0.0",
  "description": "",
  "main": "scripts/file-organizer.js",
  "type": "module",
  "private": true,
  "dependencies": {
    "@ai-sdk/google": "^2.0.12",
    "@anthropic-ai/sdk": "^0.67.0",
    "@google/genai": "^1.25.0",
    "@modelcontextprotocol/server-filesystem": "^2025.8.21",
    "@notionhq/client": "^2.2.15",
    "@types/events": "^3.0.3",
    "@types/string-similarity": "^4.0.2",
    "@voltagent/core": "^0.1.86",
    "@voltagent/vercel-ai": "^1.0.0",
    "ai": "^5.0.34",
    "chokidar": "^4.0.3",
    "dotenv": "^16.4.7",
    "fastest-levenshtein": "^1.0.16",
    "glob": "^10.4.5",
    "gray-matter": "^4.0.3",
    "ignore": "^7.0.5",
    "js-yaml": "^4.1.0",
    "sharp": "^0.34.3",
    "simple-git": "^3.28.0",
    "xlsx": "^0.18.5",
    "yargs": "^18.0.0",
    "zod": "^3.25.76"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint:md": "markdownlint --config .markdownlint.json \"**/*.md\"",
    "lint:md:fix": "markdownlint --config .markdownlint.json --fix \"**/*.md\"",
    "lint:qmd": "markdownlint --config .markdownlint.json \"**/*.qmd\"",
    "lint:qmd:fix": "markdownlint --config .markdownlint.json --fix \"**/*.qmd\"",
    "format:spacing": "tsx scripts/format-markdown-spacing.ts",
    "generate:index": "tsx scripts/generate-index.ts",
    "format:programmatic:file": "tsx scripts/review/programmatic-format-file.ts",
    "format:programmatic:all": "tsx scripts/review/programmatic-format-all-files.ts",
    "standards:apply:file": "tsx scripts/review/apply-auto-standards-file.ts",
    "standards:apply:all": "tsx scripts/review/apply-auto-standards-all.ts",
    "style:file": "tsx scripts/review/style-file.ts",
    "style:all": "tsx scripts/review/style-all-files.ts",
    "fact-check:file": "tsx scripts/review/fact-check-file.ts",
    "fact-check:all": "tsx scripts/review/fact-check-all-files.ts",
    "link-check:file": "tsx scripts/review/link-check-file.ts",
    "link-check:all": "tsx scripts/review/link-check-all-files.ts",
    "figure-check:file": "tsx scripts/review/figure-check-file.ts",
    "figure-check:all": "tsx scripts/review/figure-check-all-files.ts",
    "structure-check:file": "tsx scripts/review/structure-check-file.ts",
    "structure-check:all": "tsx scripts/review/structure-check-all-files.ts",
    "param-check:file": "tsx scripts/review/parameter-check-file.ts",
    "param-check:all": "tsx scripts/review/parameter-check-all.ts",
    "archive:file": "tsx scripts/review/archive-process-file.ts",
    "archive:all": "tsx scripts/review/archive-process-all.ts",
    "review": "tsx scripts/review/review.ts",
    "watch": "tsx scripts/watch-and-review.ts",
    "sort-references": "tsx scripts/sort-references.ts",
    "generate:references-json": "tsx scripts/generate-references-json.ts",
    "delete-empty-folders": "tsx scripts/delete-empty-folders.ts",
    "validate:frontmatter": "tsx scripts/validate-frontmatter.ts",
    "validate:latex": "python scripts/pre-render-validation.py",
    "validate:pre-render": "python scripts/pre-render-validation.py",
    "validate:render": "python scripts/post-render-validation.py",
    "fix:frontmatter": "tsx scripts/validate-frontmatter.ts --fix",
    "fix:inline-python": "python scripts/fix_inline_python.py",
    "fix:inline-python:dry-run": "python scripts/fix_inline_python.py --dry-run",
    "build": "quarto render",
    "build:validated": "npm run clean && quarto render && npm run validate:render",
    "build:html": "python scripts/render_html.py",
    "build:pdf": "python scripts/render_pdf.py",
    "build:pptx": "quarto render presentation.qmd --to pptx",
    "build:presentation": "quarto render presentation.qmd",
    "preview:presentation": "quarto preview presentation.qmd",
    "preview:book": "quarto preview .",
    "preview:dih": "quarto preview dih-index.qmd",
    "clean": "npx rimraf .quarto _book _freeze",
    "validate:all": "npm run validate:frontmatter && npm run validate:latex && npm run validate:render && npm run lint:md && npm run lint:qmd",
    "fix:all": "npm run fix:frontmatter && npm run lint:md:fix && npm run lint:qmd:fix && npm run format:spacing",
    "build:1percenttreaty": "quarto render --metadata-file _1percenttreaty.yml",
    "build:treaty": "npm run clean && npm run build:1percenttreaty",
    "build:dih": "quarto render dih-index.qmd --to html",
    "build:dih:full": "quarto render --profile dih",
    "images:analyze": "tsx scripts/generate-image-metadata.ts",
    "images:analyze:all": "tsx scripts/generate-image-metadata.ts --all",
    "images:guide": "tsx scripts/generate-image-metadata.ts --guide-only",
    "find-duplicates": "tsx scripts/find-duplicates.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-anchor": "^4.0.4",
    "@types/node": "^22.18.10",
    "@types/yargs": "^17.0.33",
    "exiftool-vendored": "^31.1.0",
    "fs-extra": "^11.3.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lightningcss": "^1.30.2",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdownlint": "^0.29.0",
    "markdownlint-cli": "^0.39.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.9.3",
    "viteval": "^0.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts",
      "**/*.test.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
